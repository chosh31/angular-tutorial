{
	"rules": {
		"indent": [
			2,
			"tab",
			{
				"SwitchCase": 1,
				"VariableDeclarator": 1
			}
		],
		"quotes": [
			2,
			"single"
		],
		"linebreak-style": [
			0,
			"unix"
		],
		"semi": [
			2,
			"always"
		],
		"space-after-keywords": [
			2,
			"always"
		],
		"space-before-blocks": [
			2,
			"always"
		],
		"brace-style": [
			2,
			"1tbs"
		]
	},
	"globals": {
	},
	"env": {
		"es6": true,
		"browser": true
	},
	"plugins": [
		"html"
	],
	"extends": "eslint:recommended"
}